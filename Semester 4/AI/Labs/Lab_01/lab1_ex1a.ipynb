    "def generate_index(probs):\n",
    "    n=len(probs)\n",
    "    array=np.arange(n)\n",
    "    index=np.random.choice(array,p=probs)\n",
    "    return index\n",
    "\n",
    "probs=[0.1, 0.3, 0.4, 0.2]\n",
    "index=generate_index(probs)\n",
    "print(\"Generated index: \",index)"
